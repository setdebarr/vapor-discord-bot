{
	"name": "vapor-discord-bot",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"lint": "prettier --check . && eslint src --ext .ts --format=pretty",
		"format": "prettier --write . && eslint src --ext .ts --fix --format=pretty",
		"start": "NODE_ENV=production node --require dotenv/config prod/index.js",
		"start:dev": "nodemon -e ts,json --exec \"npm run build:dev\"",
		"build": "tsc",
		"build:dev": "tsc && node --unhandled-rejections=strict --trace-uncaught --trace-warnings --require dotenv/config prod/index.js"
	},
	"dependencies": {
		"discord.js": "^14.11.0",
		"dotenv": "^16.1.4",
		"fs-extra": "^11.1.1"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^18.16.14",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.59.11",
		"eslint": "^8.41.0",
		"eslint-config-neon": "^0.1.47",
		"eslint-config-prettier": "^8.8.0",
		"eslint-formatter-pretty": "^5.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3"
	}
}
